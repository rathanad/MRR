<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="in.askdial.mrr.fragments.Contactus">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/contact_image"
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:layout_marginTop="16dp"
            app:srcCompat="@drawable/mrr_image" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="16dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/company_head_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/contacttxt1"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/company_contactus_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="8dp"
                android:text="@string/address" />

            <TextView
                android:id="@+id/company_telephone_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="@string/contacttxt2"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/company_contactus_telephone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="8dp"
                android:text="@string/contactno" />

            <TextView
                android:id="@+id/company_email_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="@string/contacttxt3"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/company_contactus_email"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="8dp"
                android:text="@string/emailaddress" />

            <TextView
                android:id="@+id/company_map_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="@string/contacttxt4"
                android:textSize="16sp"
                android:textStyle="bold" />

           <LinearLayout
               android:orientation="horizontal"
               android:layout_width="wrap_content"
               android:layout_height="match_parent">
            <ImageView
                android:layout_width="91dp"
                android:id="@+id/map"
                android:layout_gravity="start"
                android:src="@drawable/google_icon"
                android:layout_height="70dp"
                class="com.google.android.gms.maps.SupportMapFragment"/>

               <ImageView
                   android:layout_width="75dp"
                   android:id="@+id/youtube"
                   android:layout_gravity="start"
                   android:src="@drawable/image_youtube3"
                   android:layout_height="match_parent"
                   />
           </LinearLayout>

            <WebView
                android:id="@+id/webView_map"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                class="com.google.android.gms.youtube.SupportYoutubeFragment"></WebView>


            <WebView
                android:id="@+id/webView_youtube"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                class="com.google.android.gms.maps.SupportMapFragment"></WebView>


        </LinearLayout>
    </LinearLayout>

</android.support.v4.widget.NestedScrollView>